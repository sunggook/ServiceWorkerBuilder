{
  "name": "SW Builder",
  "version": "0.2",
  "description": "It builds service worker scripts for your site.",
  "author": "sunggch",
  "background": {
    "persistent": true,
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": {
      "16": "bible-history.jpg",
      "24": "bible-history.jpg"
    },
    "default_popup": "popup.html",
    "default_title": "Click Here"
  },
  "permissions":    [
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "storage",
    "<all_urls>",
    "tabs"
  ],
  "manifest_version": 2
}
